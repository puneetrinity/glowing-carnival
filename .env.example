# RunPod Serverless Configuration
# Copy to .env and fill in your values

# RunPod API Key (get from https://runpod.io/console/user/settings)
RUNPOD_API_KEY=your_runpod_api_key_here

# Docker Hub credentials
DOCKER_USERNAME=your_dockerhub_username
DOCKER_PASSWORD=your_dockerhub_password_or_token

# HuggingFace (optional - only if model is private)
HF_TOKEN=your_huggingface_token

# Model configuration
HF_MODEL=Puneetrinity/qwen2-7b-career
MODEL_NAME=qwen2-7b-career

# vLLM Performance Tuning
MAX_MODEL_LEN=4096
GPU_MEMORY_UTILIZATION=0.90
MAX_NUM_SEQS=8
MAX_NUM_BATCHED_TOKENS=2048

# Deployment settings
ENDPOINT_NAME=qwen-career-serverless
GPU_TYPE=NVIDIA RTX A5000
MIN_WORKERS=0
MAX_WORKERS=3
IDLE_TIMEOUT=5
EXECUTION_TIMEOUT=180
