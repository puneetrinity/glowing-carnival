{
  "title": "Chat Service â€“ Prod Overview",
  "uid": "chat-prod-overview",
  "schemaVersion": 38,
  "version": 1,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Latency P50 (ms)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(request_latency_ms_bucket{service=\"chat\"}[5m])) by (le))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Latency P95 (ms)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(request_latency_ms_bucket{service=\"chat\"}[5m])) by (le))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      },
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {
            "color": "green",
            "value": null
          },
          {
            "color": "red",
            "value": 8000
          }
        ]
      }
    },
    {
      "type": "timeseries",
      "title": "Error Rate (5xx %)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{service=\"chat\",code=~\"5..\"}[5m])) / sum(rate(http_requests_total{service=\"chat\"}[5m])) * 100"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      },
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {
            "color": "green",
            "value": null
          },
          {
            "color": "red",
            "value": 0.5
          }
        ]
      }
    },
    {
      "type": "timeseries",
      "title": "Requests per Minute",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{service=\"chat\"}[1m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/min"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Queue Depth",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum(chat_queue_pending{service=\"chat\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {
            "color": "green",
            "value": null
          },
          {
            "color": "red",
            "value": 0
          }
        ]
      }
    },
    {
      "type": "timeseries",
      "title": "Cold Starts (count)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "targets": [
        {
          "expr": "increase(chat_worker_cold_starts_total{service=\"chat\"}[5m])"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    }
  ]
}
